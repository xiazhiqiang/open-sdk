{
  "name": "datau-scene-sdk-tpl",
  "version": "1.0.2",
  "description": "场景组件工程模板",
  "repository": "git@gitlab.alibaba-inc.com:datau/toolset.git",
  "license": "MIT",
  "main": "lib/index.js",
  "unpkg": "dist/index.min.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "build": "npm run comp:doctor && npm run comp:external && npm run comp:build && npm run comp:zip && npm run docs:build",
    "comp:build": "father build",
    "comp:doctor": "father doctor",
    "comp:external": "father external",
    "comp:zip": "father zip",
    "docs:build": "dumi build",
    "docs:dev": "dumi dev",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "npm run build",
    "postpublish": "git push --tags",
    "start": "npm run docs:dev",
    "preversion": "npm run comp:external"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@umijs/lint": "^4.0.0",
    "datau-scene-docs-plugin": "^1.0.0",
    "datau-scene-sdk-plugin": "^1.0.0",
    "dumi": "^2.2.0",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM",
    "lodash": "_",
    "dayjs": "dayjs"
  },
  "library": {
    "name": [
      "datau-scene-sdk-tpl",
      "1.0.2"
    ],
    "type": "umd"
  }
}
